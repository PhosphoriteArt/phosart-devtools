{
	"name": "phosart-devtool",
	"private": true,
	"version": "0.2.29",
	"type": "module",
	"scripts": {
		"dev": "dotenvx run -- vite dev",
		"build": "vite build && node ./postbuild.mjs",
		"preview": "dotenvx run -- vite preview",
		"compiled": "dotenvx run -- PORT=3000 HOST=127.0.0.1 node build",
		"prepare": "(svelte-kit sync || echo '') && husky",
		"prepack": "vite build && node ./postbuild.mjs",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "^1.51.4",
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@types/luxon": "^3.7.1",
		"@types/node": "^22",
		"@vitest/browser-playwright": "^4.0.15",
		"concurrently": "^9.2.1",
		"cross-env": "^10.1.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"playwright": "^1.57.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.6",
		"vitest": "^4.0.15",
		"vitest-browser-svelte": "^2.0.1"
	},
	"dependencies": {
		"@atproto/api": "^0.18.9",
		"@atproto/common-web": "^0.4.8",
		"@atproto/lexicon": "^0.6.0",
		"@atproto/xrpc": "^0.7.7",
		"@ffmpeg-installer/ffmpeg": "^1.1.0",
		"@floating-ui/dom": "^1.7.4",
		"@fortawesome/fontawesome-free": "^7.1.0",
		"@monaco-editor/loader": "^1.7.0",
		"async-sema": "^3.1.1",
		"chrono-node": "^2.9.0",
		"es-toolkit": "^1.43.0",
		"fuzzysort": "^3.1.0",
		"luxon": "^3.7.2",
		"monaco-editor": "^0.55.1",
		"msgpackr": "^1.11.8",
		"opener": "^1.5.2",
		"phosart-bsky": "github:phosphoriteart/phosart-bsky#stable",
		"phosart-common": "github:phosphoriteart/phosart-common#stable",
		"sharp": "^0.34.5",
		"sharp-phash": "^2.2.0",
		"svelte-loading-spinners": "^0.3.6",
		"tslog": "^4.10.2",
		"yaml": "^2.8.2",
		"zod": "^4.3.2"
	},
	"files": [
		"build"
	],
	"main": "build/index.js",
	"bin": "build/index.js"
}
